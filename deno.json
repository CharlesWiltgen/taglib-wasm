{
  "name": "@charlesw/taglib-wasm",
  "version": "0.3.22",
  "exports": "./mod.ts",
  "publish": {
    "exclude": [
      "lib/",
      "docs/",
      "examples/",
      "scripts/",
      "tools/",
      "tests/",
      "build/taglib-build/",
      "build/*.sh",
      "build/*.cpp",
      ".github/",
      ".vscode/",
      ".ai/",
      "node_modules/",
      "dist/",
      "*.log",
      "*.tgz",
      ".DS_Store",
      "tsconfig.json",
      "deno.lock",
      "mise.toml",
      "package.json",
      "package-lock.json",
      "npm-shrinkwrap.json",
      "DENO_COMPATIBILITY_FIX.md",
      "CODEC_DETECTION.md"
    ]
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any",
        "no-unused-vars",
        "require-await"
      ]
    },
    "exclude": ["lib/", "build/", "scripts/", "examples/", "tools/", "docs/"]
  },
  "fmt": {
    "exclude": ["lib/", "build/", "docs/"]
  }
}
